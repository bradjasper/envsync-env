# Passwordless login

export PATH=$PATH:/usr/local/lib/erlang/lib/rabbitmq_server-1.5.5/sbin
export PATH=$PATH:/usr/local/mysql/bin
export PATH=~/scripts:$PATH
export PATH=/usr/local/bin:$PATH
export PATH=/usr/local/lib:$PATH
export PATH=$PATH:/opt/local/bin:/opt/local/sbi
export PATH=$PATH:/Users/bradjasper/Sites/seowiz/scripts
export PS1='\[\e[0;32m\]\u\[\e[m\] \[\e[1;34m\]\w\[\e[m\] \[\e[1;32m\]\$\[\e[m\]\[\e[1;37m\] '

export ERLANG_BIN_DIR=/opt/local/bin/
export ERLANG_INCLUDE_DIR=/opt/local/lib/erlang/usr/include/

ffpy() { sudo find . -name \*.py 2> /dev/null | xargs grep "$1" | tr -d "\t"; }
ffphp() { sudo find . -name \*.php 2> /dev/null | xargs grep "$1" | tr -d "\t"; }
ff () { sudo find . 2> /dev/null | xargs grep "$1" | tr -d "\t"; }

function gvim { /Applications/MacVim.app/Contents/MacOS/Vim -g $*; }

change_path() { export PYTHONPATH=$BASE_PATH:$HOME/Sites/$1/; }
change_project() {	export ACTIVE_PROJECT=$1; }
change_django() {
	unset DJANGO_SETTINGS_MODULE;
	export DJANGO_SETTINGS_MODULE=$1.settings;
} 
make_active() { change_project $1; change_path $1; change_django $1; }

gacp() { git add . && git commit -m "$1" && git push; }
gac() { git add . && git commit -m "$1"; }

super_kill() { kill -9 `ps -ef|grep "$1"|grep -v grep|awk '{print $2}'`; }

